    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p",
    "build:analyse": "BUNDLE_ANALYSE=true npm run build",
    "start": "cross-env NODE_ENV=development node server",
    "start-feature": "cross-env NODE_ENV=development PORT=3007 USE_OF_FORCE_URL=http://use-of-force PATHFINDER_URL=http://pathfinder USE_OF_FORCE_PRISONS=LEI OAUTH_ENDPOINT_URL=http://localhost:19090/auth/ API_ENDPOINT_URL=http://localhost:18080/ KEYWORKER_API_URL=http://localhost:18081/ PRISON_STAFF_HUB_UI_URL=http://localhost:18082/ CASENOTES_API_URL=http://localhost:18083 ALLOCATION_MANAGER_ENDPOINT_URL=http://localhost:18084/ API_WHEREABOUTS_ENDPOINT_URL=http://localhost:18082 node server | bunyan -o short",
    "start:production": "npm test && npm run build && npm start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "lint": "node node_modules/eslint/bin/eslint.js app server",
    "lint:eslint": "eslint",
    "lint:js:ci": "npm run lint:eslint -- . -f html . > eslint-report.html",
    "stylelint": "stylelint app/**/**/**/*.scss -s scss",
    "stylelint:ci": "stylelint app/**/**/**/*.scss -s scss > scss-report.txt",
    "test": "cross-env NODE_ENV=test jest --testResultsProcessor=jest-html-reporter",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "verify": "npm run lint && npm run stylelint && npm test",
    "record-build-info": "node ./bin/record-build-info",
    "plant-beanstalk": "node ./bin/plant-beanstalk.js",
    "security_audit": "npx run audit-ci --config audit-ci.json"
  },
